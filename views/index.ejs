  <body ng-app="shopHLP">

  <div class="container-fluid" ng-controller="MainController as main">
    <div id="searchBar" class="row">
      <div class="col-md-2">
        <img id="logo" src="images/hlp.png" ng-click="main.returnHome()">
      </div>
      <div class="col-md-3">
        <h3>Shop Highland Park</h3>
      </div>
      <div class="col-md-3">
        <select class="form-control" ng-model="main.selectedType" ng-options="type for type in main.types"></select>
      </div>
      <div class="col-md-3">
        <input type="search" class="form-control"  placeholder="search...">
      </div>
    </div>
<!--banner carousel -->
    <div class="row" id="slides_contril">
      <div>
        <carousel interval="myInterval">
          <slide ng-repeat="image in main.bannerImages" active="slide.active">
            <img id="banner" ng-src="{{image.image}}">
            <div class="carousel-caption">
              <h3>{{image.caption}}</h3>
                <p>{{image.caption2}}</p>
            </div>
          </slide>
        </carousel>
      </div>
    </div>


    </div>

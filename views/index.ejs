<!DOCTYPE html>
<html>
  <head>
    <title>Shop HLP</title>
    <link href="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css" rel="stylesheet">
    <link type="text/css" href="css/ui-lightness/jquery-ui-1.9.1.custom.min.css" rel="stylesheet" />
    <script src="js/jquery-1.8.2.js" type="text/javascript"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>
    <script src="js/jquery-ui-1.9.1.custom.min.js" type="text/javascript"></script>
    <script src="js/i18n/jquery-ui-i18n.min.js" type="text/javascript"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.3.10/angular.min.js"></script>
    <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.10/angular-ui-router.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/angular.js/1.2.16/angular-resource.min.js"></script>
    <script src="/javascripts/angularApp.js"></script>
    <script src="/javascripts/mainController.js"></script>
    <script src="/javascripts/businesses.service.js"></script>
    <script src="/javascripts/businessController.js"></script>
    <script src="/javascripts/events.service.js"></script>
    <script src="/javascripts/calendarController.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.9.0/moment.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/angular-moment/0.9.0/angular-moment.min.js"></script>
    <link rel='stylesheet' href='/stylesheets/style.css' />

  </head>
  <body ng-app="shopHLP">

  <div class="container-fluid" ng-controller="MainController as main">
    <div id="searchBar" class="row">
      <div class="col-md-2">
        <img id="logo" src="images/hlp.png" ng-click="main.returnHome()">
      </div>
      <div class="col-md-3">
        <h3>Shop Highland Park</h3>
      </div>
      <div class="col-md-3">
        <select class="form-control" ng-model="main.selectedType" ng-options="type for type in main.types"></select>
      </div>
      <div class="col-md-3">
        <input type="search" class="form-control"  placeholder="search...">
      </div>
    </div>
<!--banner carousel -->
    <div class="row">
      <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
          <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
          <li data-target="#myCarousel" data-slide-to="1"></li>
          <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>

        <div class="carousel-inner" role="listbox">
          <div class="item active">
            <img id="first-slide" src="images/banner.jpg">
                <div class="carousel-caption">
                  <h3>Lorem</h3>
                  <p>Something about this picture</p>
                </div>
          </div>

          <div class="item">
            <img id="second-slide" src="images/banner2.jpg">
            <div class="carousel-caption">
              <h3>Highland Park's Newest Eatery - Recess</h3>
              <p>Open Now!</p>
            </div>
          </div>

          <div class="item">
            <img id="third-slide" src="images/banner3.jpg">
            <div class="carousel-caption">
              <h3>New Brew</h3>
              <p>Coffee Gets Civil</p>
            </div>
          </div>
        </div>
      <!--controls -->

        <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
          <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
          <span class="sr-only">Prev</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
          <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
    </div>
  </div>
<!--break into partial -->
  <div class="container-fluid" ui-view>

  </div>

  </body>
</html>
